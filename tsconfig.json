{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@/types/*": [
        "./src/share/types/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
    }
  }
}
